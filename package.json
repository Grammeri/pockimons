{
  "name": "my-react-app",
  "private": true,
  "homepage": "https://Grammeri.github.io/pockimons/",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
