    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-jest": "^0.0.1",
    "whatwg-fetch": "^3.6.20"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
